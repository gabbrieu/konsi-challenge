{
    "extends": "./node_modules/gts/tsconfig-google.json",
    "ts-node": {
        "require": ["tsconfig-paths/register"]
    },
    "compilerOptions": {
        "baseUrl": ".",
        "rootDir": ".",
        "outDir": "build",
        "paths": {
            "@application/*": ["./src/application/*"],
            "@presentation/*": ["./src/presentation/*"],
            "@server": ["./src/server.ts"],
            "@utils/*": ["./src/utils/*"]
        }
    }
}
